diff -aur ncbi-cxx-toolkit-public-release-29.6.0/src/build-system/cmake/CMakeChecks.cmake rpmbuild/BUILD/ncbi-cxx-toolkit-public-release-29.6.0/src/build-system/cmake/CMakeChecks.cmake
--- ncbi-cxx-toolkit-public-release-29.6.0/src/build-system/cmake/CMakeChecks.cmake	2025-09-30 06:46:23.000000000 +0000
+++ rpmbuild/BUILD/ncbi-cxx-toolkit-public-release-29.6.0/src/build-system/cmake/CMakeChecks.cmake	2025-10-22 02:17:03.511489955 +0000
@@ -228,3 +228,46 @@
 endif()
 
 endif(NOT NCBI_PTBCFG_COLLECT_REQUIRES)
+
+# Make sure xncbi is built before certain libraries, depending on architecture,
+# OS, and compiler.
+# This code is adapted from configure.ac. It has not been tested
+# thoroughly. Some of these values are evidently for discontinued systems and
+# might not even be supported by NCBI at this point.
+message("Handling xncbi dependency issue.")
+if(("${HOST_OS}" MATCHES "SunOS" AND ("${CMAKE_SYSTEM_VERSION}" VERSION_GREATER "2.00" OR "${NCBI_COMPILER}" MATCHES "GCC")) OR
+   ("${NCBI_COMPILER}" MATCHES "Compaq") OR
+   ("${HOST_OS}" MATCHES "irix") OR
+   ("${HOST_OS}" MATCHES "AIX") OR
+   ("${HOST_OS}" MATCHES "Darwin") OR
+   ("${HOST_OS}" MATCHES "cygwin")
+)
+  set(NCBI_ATOMIC_LIB "")
+endif()
+if(("${NCBI_COMPILER}" MATCHES "GCC") OR
+   ("${NCBI_COMPILER}" MATCHES "ICC") OR
+   ("${NCBI_COMPILER}" MATCHES ".*_CLANG") OR
+  ("${NCBI_COMPILER}" MATCHES "KCC" AND "${HOST_OS}" MATCHES "linux.*"))
+    if(("${HOST_CPU}" MATCHES "i.86") OR
+       ("${HOST_CPU}" MATCHES "powerpc.*") OR
+       ("${HOST_CPU}" MATCHES "ppc.*") OR
+       ("${HOST_CPU}" MATCHES "x86_64") OR
+       ("${HOST_CPU}" MATCHES "sparc.*") OR
+       ("${HOST_CPU}" MATCHES "aarch64") OR
+      ("${HOST_CPU}" MATCHES "arm.*64.*") OR
+      ("${HOST_CPU}" MATCHES "AMD64") OR
+      ("${HOST_CPU}" MATCHES "ARM64") OR
+      ("${HOST_CPU}" MATCHES "X86"))
+      set(NCBI_ATOMIC_LIB "")
+    else()
+      set(NCBI_ATOMIC_LIB "xncbi")
+    endif()
+endif()
+
+if(NCBI_ATOMIC_LIB)
+    message("NCBI_ATOMIC_LIB set, ${NCBI_ATOMIC_LIB}")
+else()
+  message("NCBI_ATOMIC_LIB not set, ${NCBI_ATOMIC_LIB}")
+endif()
+  
+
